{
    "input_model":{
        "type": "PyTorchModel",
        "config": {
            "model_loader": "load_pytorch_origin_model",
            "model_script": "user_script.py",
            "io_config": {
                "input_names": ["input_ids", "attention_mask", "token_type_ids"],
                "input_shapes": [[1, 128], [1, 128], [1, 128]],
                "input_types": ["int64", "int64", "int64"],
                "output_names": ["output"]
            }
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": ["CPU"]
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics":[
                {
                    "name": "accuracy",
                    "type": "custom",
                    "sub_types": [
                        {"name": "accuracy_custom", "priority": 1, "higher_is_better": true}
                    ],
                    "user_config":{
                        "user_script": "user_script.py",
                        "evaluate_func": "eval_accuracy",
                        "batch_size": 1
                    }
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [{"name": "avg", "priority": 2}],
                    "user_config":{
                        "user_script": "user_script.py",
                        "dataloader_func": "create_dataloader",
                        "batch_size": 1
                    }
                }
            ]
        }
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 13
            }
        },
        "transformers_optimization": {
            "type": "OrtTransformersOptimization",
            "config": {"model_type": "bert"}
        },
        "quantization": {
            "type": "IncQuantization",
            "disable_search": true,
            "config": {
                "approach": "SEARCHABLE_VALUES",
                "user_script": "user_script.py",
                "dataloader_func": "inc_glue_calibration_reader",
                "evaluate_func": "eval_accuracy",
                "accuracy_criterion": {
                    "tolerable_loss": 0.02
                }
            }
        }
    },
    "engine": {
        "log_severity_level": 0,
        "search_strategy": {
            "execution_order": "joint",
            "search_algorithm": "exhaustive"
        },
        "evaluator": "common_evaluator",
        "host": "local_system",
        "target": "local_system",
        "clean_cache": true,
        "cache_dir": "cache"
    }
}
